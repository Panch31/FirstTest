<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="testng" verbose="1">
    <listeners>
        <listener class-name="util.TestListener"></listener>
    </listeners>
    <test name="PunchTest">
        <classes>
            <class name="FirstTest">
                <methods>
                    <exclude name="linkTest"/>
                    <exclude name="loginTest"/>
                    <exclude name="repoTest"/>
                    <exclude name="marketplaceTest"/>
                    <exclude name="marketPlaceLinkTest"/>
                    <exclude name="dataProviderTest"/>
                    <exclude name="loginDataProviderTest"/>
                    <exclude name="profileTest"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>
